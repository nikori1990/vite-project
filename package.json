{
    "name": "vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "test": "vite --mode test",
        "prod": "vite --mode production",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "pnpm run build && vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,css,json,tsx,html,vue,less,md}\"",
        "lint:style": "stylelint --fix \"**/*.{css,tsx,html,vue,less,scss}\" --cache --cache-location node_modules/.cache/stylelint",
        "lint:ls-lint": "ls-lint",
        "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
        "lint:pretty": "pretty-quick --staged",
        "install:husky": "is-ci || husky install",
        "postinstall": "npm run install:husky",
        "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-config.cjs"
    },
    "dependencies": {
        "algoliasearch": "^4.14.2",
        "animate.css": "^4.1.1",
        "axios": "^1.2.1",
        "gsap": "^3.11.3",
        "instantsearch.css": "^7.4.5",
        "js-cookie": "^3.0.1",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.27",
        "vue": "^3.2.45",
        "vue-instantsearch": "^4.6.0",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@element-plus/icons-vue": "^2.0.10",
        "@iconify-json/ep": "^1.1.8",
        "@ls-lint/ls-lint": "^1.11.2",
        "@types/ityped": "^1.0.1",
        "@types/js-cookie": "^3.0.2",
        "@types/nprogress": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vitejs/plugin-vue-jsx": "^2.1.1",
        "@vueuse/core": "^9.6.0",
        "autoprefixer": "^10.4.13",
        "conventional-changelog-cli": "^2.2.2",
        "element-plus": "^2.2.26",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-define-config": "^1.12.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.8.0",
        "husky": "^8.0.2",
        "is-ci": "^3.0.1",
        "ityped": "^1.0.3",
        "lint-staged": "^13.1.0",
        "mockjs": "^1.1.0",
        "naive-ui": "^2.34.2",
        "pinia-persistedstate-plugin": "^0.1.0",
        "postcss": "^8.4.19",
        "postcss-html": "^1.5.0",
        "postcss-loader": "^7.0.2",
        "postcss-scss": "^4.0.6",
        "prettier": "^2.8.1",
        "pretty-quick": "^3.1.3",
        "sass": "^1.56.2",
        "sass-loader": "^13.2.0",
        "stylelint": "^14.16.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-order": "^5.0.0",
        "typescript": "^4.9.4",
        "unplugin-auto-import": "^0.12.0",
        "unplugin-element-plus": "^0.4.1",
        "unplugin-icons": "^0.14.14",
        "unplugin-vue-components": "^0.22.11",
        "vite": "^3.2.5",
        "vite-plugin-inspect": "^0.7.9",
        "vite-plugin-mock": "^2.9.6",
        "vite-plugin-pages": "^0.27.1",
        "vite-plugin-svg-icons": "^2.0.1",
        "vite-plugin-vue-layouts": "^0.7.0",
        "vite-plugin-windicss": "^1.8.8",
        "vue-eslint-parser": "^9.1.0",
        "vue-request": "1.2.4",
        "vue-tsc": "^1.0.12",
        "webpack": "^5.75.0",
        "windicss": "^3.5.6"
    }
}
