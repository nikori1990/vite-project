{
    "name": "vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "test": "vite --mode test",
        "prod": "vite --mode production",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "pnpm run build && vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,css,json,tsx,html,vue,less,md}\"",
        "lint:style": "stylelint --fix \"**/*.{css,tsx,html,vue,less,scss}\" --cache --cache-location node_modules/.cache/stylelint",
        "lint:ls-lint": "ls-lint",
        "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
        "lint:pretty": "pretty-quick --staged",
        "install:husky": "is-ci || husky install",
        "postinstall": "npm run install:husky",
        "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-config.cjs"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.0.9",
        "animate.css": "^4.1.1",
        "element-plus": "^2.2.17",
        "gsap": "^3.11.2",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.22",
        "vue": "^3.2.37",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@iconify-json/ep": "^1.1.7",
        "@ls-lint/ls-lint": "^1.11.2",
        "@types/ityped": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "@vitejs/plugin-vue": "^3.1.0",
        "@vitejs/plugin-vue-jsx": "^2.0.1",
        "@vueuse/core": "^9.3.0",
        "autoprefixer": "^10.4.12",
        "conventional-changelog-cli": "^2.2.2",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-define-config": "^1.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.5.1",
        "husky": "^8.0.1",
        "is-ci": "^3.0.1",
        "ityped": "^1.0.3",
        "lint-staged": "^13.0.3",
        "naive-ui": "^2.33.3",
        "postcss": "^8.4.16",
        "postcss-html": "^1.5.0",
        "postcss-loader": "^7.0.1",
        "postcss-scss": "^4.0.5",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "sass": "^1.55.0",
        "sass-loader": "^13.0.2",
        "stylelint": "^14.12.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^28.0.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-order": "^5.0.0",
        "typescript": "^4.6.4",
        "unplugin-auto-import": "^0.11.2",
        "unplugin-element-plus": "^0.4.1",
        "unplugin-icons": "^0.14.11",
        "unplugin-vue-components": "^0.22.7",
        "vite": "^3.1.0",
        "vite-plugin-pages": "^0.26.0",
        "vite-plugin-vue-layouts": "^0.7.0",
        "vite-plugin-windicss": "^1.8.8",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^0.40.4",
        "webpack": "^5.74.0",
        "windicss": "^3.5.6"
    }
}
